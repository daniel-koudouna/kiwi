               <children>
                  <FlowPane fx:id="tilePane" focusTraversable="true" maxHeight="1.7976931348623157E308" minHeight="0.0" pickOnBounds="false" HBox.hgrow="ALWAYS" />
                  <Pane fx:id="scrollBar" maxHeight="1.7976931348623157E308" minWidth="-Infinity" onMouseClicked="#onScrollbarClick" pickOnBounds="false" prefHeight="200.0" prefWidth="20.0" scaleY="1.01" style="-fx-background-color: gray;">
                     <children>
                        <Pane fx:id="scrollControl" onMouseDragged="#onScrollControlDragged" prefHeight="40.0" prefWidth="20.0" style="-fx-background-color: white;" />
                     </children>
                  </Pane>
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox pickOnBounds="false">
