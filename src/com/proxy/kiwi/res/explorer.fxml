              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <TextField fx:id="searchBox" promptText="Search" />
                  <Button mnemonicParsing="false" text="+" GridPane.columnIndex="1" />
                  <Button mnemonicParsing="false" text="Settings" GridPane.columnIndex="3" />
                  <FlowPane fx:id="filterPane" GridPane.columnIndex="2" />
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </GridPane>
         </children>
      </VBox>
   </children>
